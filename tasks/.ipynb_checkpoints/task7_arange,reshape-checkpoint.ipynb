    "#task: arange,reshape related\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "a = np.arange(18)\n",
    "print(a, \"\\n\")\n",
    "b = np.arange(20,38)\n",
    "print(b, \"\\n\")\n",
    "\n",
    "a = a.reshape((6,3))\n",
    "b = b.reshape((6,3))\n",
    "print(a, \"\\n\")\n",
    "print(b, \"\\n\")\n",
    "\n",
    "sum = a+b\n",
    "print(\"the sum matrix: \\n\", sum)\n"
   ]
